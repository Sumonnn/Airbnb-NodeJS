<% layout('/layouts/boilerplate') -%>

<body>
  <h3>All Listing</h3>
  <form action="/listings/new" method="get">
    <button>Create New Listing</button>
  </form>
  <ul>
    <% allListing.forEach((listing)=> { %>
      <li><a href="/listings/<%= listing._id %>">
          <%= listing.title %>
        </a></li>
      <% }) %>
  </ul>
</body>
